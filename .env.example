# SonarQube Configuration
SONARQUBE_URL=https://sonarqube.example.com
SONARQUBE_TOKEN=your_sonarqube_token
SONARQUBE_PROJECT_KEY=your_project_key

# Git Configuration
GIT_REPO_URL=https://github.com/your-org/your-repo.git
GIT_USERNAME=your_git_username
GIT_PASSWORD=your_git_password
GIT_EMAIL=ai-sonar-fixer@example.com
GIT_NAME=AI Sonar Fixer
GIT_MASTER_BRANCH=master

# Azure DevOps Configuration
AZURE_DEVOPS_ORG=your_azure_devops_org
AZURE_DEVOPS_PROJECT=your_azure_devops_project
AZURE_DEVOPS_TOKEN=your_azure_devops_token
AZURE_DEVOPS_REPO_ID=your_azure_devops_repo_id

# Gemini AI Configuration
GEMINI_API_KEY=your_gemini_api_key

# Jenkins Configuration
JENKINS_URL=https://jenkins.example.com
JENKINS_JOB_NAME=your_jenkins_job_name
JENKINS_USERNAME=your_jenkins_username
JENKINS_API_TOKEN=your_jenkins_api_token

# Application Configuration
LOG_LEVEL=INFO
TEMP_DIR=/tmp/ai-sonar-fixer
MAX_ISSUES_PER_RUN=50
CONTEXT_LINES_BEFORE=10
CONTEXT_LINES_AFTER=10
RETRY_ATTEMPTS=3
RETRY_DELAY=5
